#    Object Properties
#
#################################################################


###  http://www.example.org/test.ttl#r

:r rdf:type owl:ObjectProperty .



###  http://www.example.org/test.ttl#s

:s rdf:type owl:ObjectProperty ;
   
   rdfs:subPropertyOf :r .



###  http://www.example.org/test.ttl#t

:t rdf:type owl:FunctionalProperty ,
            owl:ObjectProperty ;
   
   rdfs:subPropertyOf :s .





#################################################################
