#    Classes
#
#################################################################


###  http://www.example.org/test.ttl#ClassA

:ClassA rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassB

:ClassB rdf:type owl:Class ;
        
        rdfs:subClassOf :ClassA .



###  http://www.example.org/test.ttl#ClassC

:ClassC rdf:type owl:Class ;
        
        rdfs:subClassOf :ClassB .



###  http://www.example.org/test.ttl#ClassD

:ClassD rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassE

:ClassE rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassF

:ClassF rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassG

:ClassG rdf:type owl:Class ;
        
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:intersectionOf ( :ClassD
                                                   :ClassE
                                                   :ClassF
                                                 )
                            ] .



###  http://www.example.org/test.ttl#ClassH

:ClassH rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassI

:ClassI rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassJ

:ClassJ rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassK

:ClassK rdf:type owl:Class .



###  http://www.example.org/test.ttl#ClassL

:ClassL rdf:type owl:Class ;
        
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :ClassH
                                            :ClassI
                                            :ClassJ
                                            :ClassK
                                          )
                            ] .





